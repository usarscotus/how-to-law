{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client"],
    "baseUrl": "./",
    "paths": {
      "~/*": ["src/*"]
    },
    "resolveJsonModule": true
  },
  "include": ["src", "astro.config.mjs"],
  "exclude": ["node_modules", "dist"]
}
